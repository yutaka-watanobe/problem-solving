#include<iostream>
#include<algorithm>
using namespace std;

int A[9], U[10], cnt;

void rec(int p){
  if ( p == 9 ){
    //    for ( int i = 1; i <= 9; i++ ) if ( !U[i] ) return;
    if ( ((A[0]+A[2]+A[5])%10 == A[8]) &&
	 (((A[0]+A[2]+A[5])/10+A[1]+A[4])%10 == A[7]) &&
	 (((A[0]+A[2]+A[5])/10+A[1]+A[4])/10 + A[3] == A[6]) ) cnt++;
    return;
  }
  if ( A[p] != -1 ){
    U[A[p]] = true;
    rec(p+1);
    //    U[A[p]] = false;
  } else {
    for ( int i = 1; i <= 9; i++ ){
      if ( U[i] ) continue;
      U[i] = true;
      A[p] = i;
      rec(p+1);
      A[p] = -1;
      U[i] = false;
    }
  }
}

int main(){
  for ( int i = 0; i < 9; i++ ) cin >> A[i];
  cnt = 0;
  rec(0);
  cout << cnt << endl;
  return 0;
}
